    "type": "git",
    "url": "git+https://github.com/blackbaud/skyux-i18n.git"
  },
  "bugs": {
    "url": "https://github.com/blackbaud/skyux-i18n/issues"
  },
  "homepage": "https://github.com/blackbaud/skyux-i18n#readme",
  "dependencies": {
    "@angular/animations": "7.0.2",
    "@angular/common": "7.0.2",
    "@angular/compiler": "7.0.2",
    "@angular/core": "7.0.2",
    "@angular/forms": "7.0.2",
    "@angular/http": "7.0.2",
    "@angular/platform-browser": "7.0.2",
    "@angular/platform-browser-dynamic": "7.0.2",
    "@angular/router": "7.0.2",
    "@skyux/assets": "3.0.0",
    "@skyux/avatar": "3.0.0-rc.1",
    "@skyux/config": "3.1.0",
    "@skyux/core": "3.2.2",
    "@skyux/errors": "3.0.0-rc.1",
    "@skyux/forms": "3.0.0-rc.1",
    "@skyux/http": "3.0.0",
    "@skyux/i18n": "3.2.1",
    "@skyux/indicators": "3.0.0-rc.2",
    "@skyux/modals": "3.0.0-rc.3",
    "@skyux/omnibar-interop": "3.0.0",
    "@skyux/router": "3.0.0",
    "@skyux/theme": "3.2.2",
    "core-js": "2.5.7",
    "rxjs": "6.3.3",
