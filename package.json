{
  "name": "skyux-i18n-demo",
  "version": "3.2.1",
  "description": "SKY UX Internationalization",
  "main": "bundles/bundle.umd.js",
  "module": "index.ts",
  "scripts": {
    "build": "skyux build-public-library",
    "build:dev-bundle": "npm run build && node ./scripts/dev-bundle.js",
    "skyux": "skyux",
    "start": "npm run build:dev-bundle && skyux serve --hmr",
    "test": "npm run build:dev-bundle && skyux test --coverage library --logFormat none",
    "test:ci": "npm run build:dev-bundle && skyux test --platform travis --coverage library && npm run build",
    "test:visual": "npm run build:dev-bundle && skyux e2e --logFormat none"
  },
